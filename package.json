{
  "name": "backlog-api",
  "description": "Backlog API wrapper for Node.js",
  "version": "1.0.2",
  "author": {
    "email": "m@bouzuya.net",
    "name": "bouzuya",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/node-backlog-api/issues"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^9.3.0",
    "@types/power-assert": "^1.4.29",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^4.1.3",
    "babel-cli": "^6.26.0",
    "babel-preset-power-assert": "^2.0.0",
    "beater": "^5.0.1",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.2",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.6",
    "tap-dot-b": "^0.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "watch": "^1.0.2"
  },
  "homepage": "https://github.com/bouzuya/node-backlog-api",
  "keywords": [
    "backlog",
    "nulab"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/node-backlog-api.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:lint' 'build:es2015' 'build:es5' 'build:lib'",
    "build:es2015": "tsc",
    "build:es5": "babel -q -d '.tmp/es5/' '.tmp/es2015/'",
    "build:lib": "cpx '.tmp/es2015/src/**/*.{js,ts}' 'lib/'",
    "build:lint": "tslint --project tsconfig.json",
    "clean": "rimraf '.tmp' 'lib'",
    "prepare": "npm-run-all -s 'clean' 'build'",
    "test": "node .tmp/es5/test/index.js | tap-dot-b",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run build:es2015 -- --watch",
    "watch:es5": "npm run build:es5 -- --watch",
    "watch:test": "watch 'npm run test' '.tmp/es5/'"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
