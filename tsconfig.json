{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./.tmp/es2015/",
    "removeComments": true,
    "rootDir": "./",
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "es2015"
  },
  "files": [
    "src/index.ts",
    "test/index.ts"
  ]
}
